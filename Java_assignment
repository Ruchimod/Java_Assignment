package Java_Assignment;

public class Q12_ForWhile {

	public static void main(String[] args) {
		
		for (int i = 1; i <= 100; i++) {
            System.out.println("Using for loop: "+i);
        }
		
		int count1 = 1;
        while (count1 <= 100) {
            System.out.println("Using while loop: " +count1);
            count1++;
        }
    }

	}
