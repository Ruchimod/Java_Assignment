package Java_Assignment;

public class Q17_EvenOdd {

	public static void main(String[] args) {
		
		int i = 0;
		
		for (i=0;i<100;i++) {
		
			if ( i%2==0)
			{
				System.out.println("Even number: "+i);
			
			}
			else 
			{
				System.out.println("Odd number: "+i);
			}
		}
		
		
			
			

	}

}
